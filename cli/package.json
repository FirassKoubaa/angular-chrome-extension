{
  "name": "angular-chrome-extension-cli",
  "shortName": "ng-chrome",
  "version": "1.0.0",
  "description": "CLI tool to generate angular chrome extensions",
  "repository": {
    "type": "git",
    "url": "https://github.com/larscom/angular-chrome-extension.git"
  },
  "main": "./lib/main.js",
  "bin": {
    "ng-chrome": "./lib/main.js"
  },
  "scripts": {
    "build": "cpx package.json lib && tsc -p .",
    "start": "npm run build -- --watch",
    "tscov": "tscov --min-coverage 95",
    "tscov:details": "tscov --details"
  },
  "author": "Lars Kniep",
  "license": "MIT",
  "dependencies": {
    "clear": "^0.1.0",
    "figlet": "^1.3.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.6",
    "inversify": "^5.0.1",
    "kleur": "^3.0.3",
    "nodegit": "^0.26.5",
    "npm": "^6.14.2",
    "ora": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@liftr/tscov": "^1.4.5",
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^13.9.0",
    "@types/nodegit": "^0.26.1",
    "@types/npm": "^2.0.31",
    "@types/ora": "^3.2.0",
    "cpx": "^1.5.0",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3"
  }
}
